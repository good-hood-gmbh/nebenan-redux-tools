{
  "name": "nebenan-redux-tools",
  "description": "A set of redux helpers",
  "author": "Nebenan.de <max@nebenan.de>",
  "license": "SEE LICENSE IN LICENSE FILE",
  "readmeFilename": "README.md",
  "homepage": "https://nebenan.de/",
  "repository": "good-hood-gmbh/nebenan-redux-tools",
  "bugs": "https://github.com/good-hood-gmbh/nebenan-redux-tools/issues",
  "version": "4.5.0",
  "engines": {
    "node": "^10.x.x"
  },
  "scripts": {
    "reinstall": "rm -rf node_modules package-lock.json && npm install",
    "check": "npm test && npm run lint && echo \"\\033[0;32mReady to publish\\033[0m\"",
    "clean": "rm -rf ./lib/ || exit 1",
    "build:babel": "NODE_ENV=production babel src --out-dir lib --copy-files",
    "prepublishOnly": "npm run check && npm run clean && npm run build:babel",
    "lint": "eslint --ext .es,.js ./",
    "lint:tofile": "eslint --ext .es,.js --no-color -o ./eslint-report.log ./",
    "test": "npm run build:babel && TZ='GMT' mocha \"test/**/*.js\"",
    "test:tofile": "npm run build:babel && TZ='GMT' mocha --no-colors -R spec \"test/**/*.js\" 2>&1 | tee test-report.log"
  },
  "devDependencies": {
    "@babel/cli": "^7.5.5",
    "@babel/core": "^7.5.5",
    "@babel/plugin-proposal-json-strings": "^7.0.0",
    "@babel/plugin-transform-runtime": "^7.5.5",
    "@babel/plugin-transform-strict-mode": "^7.0.0",
    "@babel/preset-env": "^7.5.5",
    "@babel/runtime": "^7.5.5",
    "axios": "^0.19.0",
    "babel-plugin-add-module-exports": "^1.0.2",
    "chai": "^4.2.0",
    "escape-string-regexp": "1.0.5",
    "eslint": "^6.2.2",
    "eslint-config-airbnb-base": "^14.0.0",
    "eslint-plugin-import": "^2.18.2",
    "local-storage-adapter": "^2.0.0",
    "lodash": "^4.17.15",
    "mocha": "^6.2.0",
    "proxyquire": "^2.1.3",
    "qs": "^6.8.0",
    "sinon": "^7.4.1",
    "updeep": "^1.1.0"
  },
  "peerDependencies": {
    "@babel/runtime": "^7.5.5",
    "axios": "^0.19.0",
    "lodash": "^4.17.15",
    "updeep": "^1.1.0",
    "local-storage-adapter": "^2.0.0",
    "escape-string-regexp": "1.0.5",
    "qs": "^6.7.0"
  },
  "dependencies": {
    "js-cookie": "^2.2.1"
  }
}
